<!DOCTYPE html>
<html lang="en">
<%- include('./partials/head.ejs') %>
<body>
   <%- include('./partials/navbar.ejs') %> 

   <div class="blogs">
      <% if(blogs.length > 0) { %>
         <% blogs.forEach(blog => { %>
            <div class="blog">
               <h2 class="blog-title"><%= blog.title %></h2>
               <p class="blog-body"><%= blog.body%></p>
            </div>
         <% }) %>
      <% } %>
   </div>
</body>
</html>